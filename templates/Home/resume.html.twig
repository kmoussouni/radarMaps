                    <div class="columns">
                        <div class="column is-two-fifths">
                            <h1 class="title is-size-1">Karim Moussouni</h1>
                            <p class="subtitle">Full Stack Developer</p>
                        </div>
                        <div class="column is-2"><figure class="image image is-128x128">
                                <img class="is-rounded" src="{{ asset('build/img/profile.jpg')}} ">
                            </figure>
                        </div>
                        <div class="column has-text-grey-light has-text-right-in-desktop">
                            <p class="has-text-weight-light">Paris, France</p>
                            <p class="has-text-weight-light">(+62) 857-8123-8798</p>
                            <p class="has-text-weight-light">karim.moussouni@gmail.com</p>
                            <div class="social-icon">
                                <a href="https://twitter.com/kmoussouni" class="twitter" title="Twitter">
                                    <i class="fab fa-twitter"></i>
                                    @kmoussouni
                                </a>
                            </div>
                            <div class="social-icon">
                                <a href="https://github.com/kmoussouni" class="github" title="Github">
                                    <i class="fab fa-github"></i>
                                    kmoussouni
                                </a>
                            </div>
                            <div class="social-icon">
                                <a href="https://www.linkedin.com/in/karim-moussouni-39407868/" class="github" title="Github">
                                    <i class="fab fa-linkedin"></i>
                                    kmoussouni
                                </a>
                            </div>
                        </div>
                    </div>
            </section>

            <div class="hr"></div>

            <section class="section">
                <div class="columns">
                    <div class="column is-three-fifths">
                        <div class="summary">
                            <div class="title is-size-8 has-text-primary has-text-weight-bold">{{ 'resume.content.title'|trans }}</div>
                            <div class="wrapper">
                                <p class="has-text-justified">
                                    {{ 'resume.content.label'|trans }}
                                </p>
                                <br />
                            </div>
                        </div>
                        <div class="experience">
                            <div class="title is-size-8 has-text-primary has-text-weight-bold">{{ 'resume.experience.title'|trans }}</div>
                            {% for experience in experiences %}
                                <div class="item">
                                    <div class="is-size-8">
                                        <b>{{ experience.title }},</b>
                                        <span class="has-text-weight-semi-bold"><i>{{ experience.client.name }}</i></span>
                                    </div>
                                    <div class="is-size-8">{{ experience.startDate|date('M Y') }} - {{ experience.endDate|date('M Y') }} - {{ experience.client.location }}</div>
                                    <ul style="list-style:disc;margin-left:1em">
                                        <p class="has-text-justified is-size-8">{{ experience.body }}</p>
                                    </ul>
                                </div>
                            {% endfor %}
                        </div>
                        <div class="education">
                            <div class="title is-size-8 has-text-primary has-text-weight-bold">{{ 'resume.education.title'|trans }}</div>
                            <div class="item">
                                <div class="is-size-8">
                                    <span class="has-text-weight-semi-bold">
                                        <i>
                                            <b>BTS Informatique de Gestion</b>,
                                            1999 - 2001
                                        </i>
                                    </span>
                                </div>
                                <div class="is-size-8">
                                </div>
                                <div>
                                    Cogéfi, Champs sur Marne
                                </div>
                            </div>
                            <div class="item">
                                <div class="is-size-8">
                                    <span class="has-text-weight-semi-bold">
                                        <i>
                                            <b>MIAS / Mathématiques et Informatique Appliquées aux Sciences</b>,
                                            1997 – 1999
                                        </i>
                                    </span>
                                </div>
                                <div class="is-size-8"></div>
                                <div>
                                    Université de Marne-la-Vallée, Cités Descartes
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="column">
                        <div class="projects">
                            <div class="title is-size-8 has-text-primary has-text-weight-bold">{{ 'resume.projects.title'|trans }}</div>
                            {% for project in projects %}
                                <div class="item">
                                    <div class="is-size-8">
                                        <b>{{ project.title }}, {{ project.body|truncate }}</b> — <i>{{ project.body }}</i>
                                    </div>
                                    <div class="is-size-8">{{ project.createdAt|date('M Y') }}</div>
                                </div>
                            {% endfor %}
                        </div>
                        <div class="skills">
                            <div class="title is-size-8 has-text-primary has-text-weight-bold">{{ 'resume.skills.title'|trans }}</div>
                            <div class="wrapper">
                                <div class="item">
                                    <div>{{ 'resume.skills.javascript.title'|trans }}</div>
                                    <progress class="progress is-primary is-small" value="60" max="100">60%</progress>
                                </div>
                                <div class="item">
                                    <div>{{ 'resume.skills.symfony.title'|trans }}</div>
                                    <progress class="progress is-primary is-small" value="70" max="100">70%</progress>
                                </div>
                                <div class="item">
                                    <div>{{ 'resume.skills.php.title'|trans }}</div>
                                    <progress class="progress is-primary is-small" value="80" max="100">80%</progress>
                                </div>
                                <div class="item">
                                    <div>{{ 'resume.skills.reactjsreduc.title'|trans }}</div>
                                    <progress class="progress is-primary is-small" value="50" max="100">50%</progress>
                                </div>
                                <div class="item">
                                    <div>{{ 'resume.skills.docker.title'|trans }}</div>
                                    <progress class="progress is-primary is-small" value="70" max="100">70%</progress>
                                </div>
                                <div class="item">
                                    <div>{{ 'resume.skills.elasticsearch.title'|trans }}</div>
                                    <progress class="progress is-primary is-small" value="70" max="100">70%</progress>
                                </div>
                            </div>
                        </div>
                        <div class="languages">
                            <div class="title is-size-8 has-text-primary has-text-weight-bold">
                                {{ 'resume.languages.title'|trans }}
                            </div>
                            <div class="wrapper">
                                {{ 'resume.languages.english.label'|trans }}, {{ 'resume.languages.french.label'|trans }},
                            </div>
                        </div>
                    </div>
                </div>
            </section>

        </article>
    </div>
</div>
